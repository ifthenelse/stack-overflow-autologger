{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "no-console": "warn",
        "no-unused-vars": "warn",
        "prefer-const": "error"
    },
    "ignorePatterns": [
        "node_modules/",
        "dist/",
        "*.min.js"
    ]
}